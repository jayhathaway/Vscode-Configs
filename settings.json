{
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "git.openRepositoryInParentFolders": "always",
  "autoDocstring.docstringFormat": "sphinx-notypes",
  "remote.autoForwardPortsSource": "hybrid",
  "cmake.configureOnOpen": true,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "cmake.showOptionsMovedNotification": false,
  "editor.fontLigatures": false,
  "editor.fontVariations": false,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "workbench.colorCustomizations": {
    "terminal.background": "#00000000"
  },
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "vscode_vibrancy.opacity": 0,
  "vscode_vibrancy.refreshInterval": 100,
  "window.titleBarStyle": "custom",
  "workbench.sideBar.location": "right",
  "workbench.iconTheme": "Monokai Pro Icons",
  "vim.smartRelativeLine": true,
  "editor.lineNumbers": "relative",
  "glassit.alpha": 220,
  "telemetry.telemetryLevel": "off",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "security.allowedUNCHosts": ["wsl.localhost"],
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "CodeGPT.apiKey": "OpenAI",
  "workbench.colorTheme": "Dracula Dark",
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "Dracula Dark",
  "workbench.preferredLightColorTheme": "Dracula Light",
  "workbench.preferredHighContrastLightColorTheme": "Dracula Light",
  "workbench.preferredHighContrastColorTheme": "Dracula Dark",
  "window.systemColorTheme": "auto"
}
